p <- c(0, 1, 0.2)
gp.sim <- revd(n = 50, loc = p[1], scale = p[2], shape = p[3], type = "GP")
plot( gp.sim)
gp.sim <- cbind(1:50, gp.sim)
colnames(gp.sim) <- c("obs", "gp.sim")
gp.sim <- as.in2extRemesDataObject( gp.sim)
gp.sim[["name"]] <- "GP Simulated"
gp.sim[["params"]] <- c(0, 1, 0.2)
gp.sim[["generated"]] <- TRUE
assign( "te", gp.sim, pos=".GlobalEnv")
save.image()
dd <- get( "te")
var.val <- dd[["data"]][, ]
dd <- get( "te")
var.val <- dd[["data"]][, 2]
threshrange.plot(x = var.val, r = c(0, 2), type = "GP", nint = 5, na.action = na.fail)
